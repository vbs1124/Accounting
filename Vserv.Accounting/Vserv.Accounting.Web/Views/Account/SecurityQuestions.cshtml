@model IEnumerable<Vserv.Accounting.Data.Entity.SecurityQuestion>

@{
    ViewBag.Title = "Security Questions";
}

<ul class="nav nav-tabs">
    <li class="active"><a href="#add-employee" data-toggle="tab">@ViewBag.Title</a></li>
</ul>
<div class="tab-content">
    <div class="tab-pane fade active in padding-top10" id="add-employee">
        <div class="row">
            <div class="col-lg-12">
                <div class="panel panel-default">
                    <!-- /.panel-heading -->
                    <div class="panel-body">
                        <div class="dataTable_wrapper">
                            <table class="table table-striped table-bordered table-hover" id="dataTables-security-questions">
                                <thead>
                                    <tr>
                                        <th>#</th>
                                        <th>Question</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                        <tr class="odd gradeX">
                                            <td>@item.SecurityQuestionId</td>
                                            <td>@item.Question</td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                        <!-- /.table-responsive -->
                    </div>
                    <!-- /.panel-body -->
                </div>
                <!-- /.panel -->
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <!-- /.row -->
    </div>
</div>
