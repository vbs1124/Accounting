@model Vserv.Accounting.Web.Models.EmployeeModel
@{
    ViewBag.Title = "Edit Employee";
}

<ul class="nav nav-tabs">
    <li class="active"><a href="#edit-employee" data-toggle="tab">@ViewBag.Title</a></li>
    <li class=""><a href="#emp-salary-summary" data-toggle="tab" aria-expanded="false">Salary Summary</a></li>
    <li class=""><a href="#emp-salary-breakup" data-toggle="tab" aria-expanded="false">Salary Breakup</a></li>
    <li class=""><a href="#employee-history" data-toggle="tab" aria-expanded="false">View History</a></li>
</ul>
<div class="tab-content">

    <div class="tab-pane fade active in padding-top10" id="edit-employee">
        @using (Html.BeginForm("edit", "employee"))
        {
            @Html.HiddenFor(emp => emp.EmployeeId)
            @Html.Partial("_EmployeePartial")
        }
    </div>
    <div class="tab-pane fade padding-top10" id="emp-salary-summary">
        @using (Html.BeginForm("calculateBreakup", "employee"))
        {
        <div class="row">
            <div class="col-md-3">
                <div class="form-group">
                    <label class="control-label" for="">Cost to company</label>
                    <div class="input-group">
                        <span class="input-group-addon"><i class="fa fa-inr"></i></span>
                        <input type="text" class="form-control">
                    </div>
                </div>
                <div class="form-group">
                    <label class="control-label" for="">Cab Deductions</label>
                    <div class="input-group">
                        <span class="input-group-addon"><i class="fa fa-inr"></i></span>
                        <input type="text" class="form-control">
                    </div>
                </div>
                <div class="form-group">
                    <label class="control-label" for="">Project Incentive</label>
                    <div class="input-group">
                        <span class="input-group-addon"><i class="fa fa-inr"></i></span>
                        <input type="text" class="form-control">
                    </div>
                </div>
                <div class="form-group">
                    <label class="control-label" for="">Car Lease</label>
                    <div class="input-group">
                        <span class="input-group-addon"><i class="fa fa-inr"></i></span>
                        <input type="text" class="form-control">
                    </div>
                </div>
                <div class="form-group">
                    <label class="control-label" for="">Food Coupons</label>
                    <select class="form-control" id="select">
                        <option>--SELECT--</option>
                        <option>1100</option>
                        <option>2200</option>
                    </select>
                </div>
                <div class="form-group">
                    <label class="control-label" for="">Performance Incentive</label>
                    <select class="form-control" id="select">
                        <option>--SELECT--</option>
                        <option>5 % of CTC paid Anually</option>
                        <option>25% of increment in June and 25% in December</option>
                    </select>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-3">
                <button type="submit" class="btn btn-primary pull-right">Calculate Breakup</button>
            </div>
        </div>
        }
    </div>

