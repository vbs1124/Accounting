@model Vserv.Accounting.Web.Models.DesignationModel

@{
    ViewBag.Title = "Edit Designation";
}

@using (Html.BeginForm("edit", "designation"))
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)
    @Html.HiddenFor(model => model.DesignationId)
    @Html.HiddenFor(model => model.Code)
    @Html.HiddenFor(model => model.CreatedDate)
    @Html.HiddenFor(model => model.DisplayOrder)
    @Html.HiddenFor(model => model.CreatedBy)
    @Html.Partial("_DesignationPartial")
}
